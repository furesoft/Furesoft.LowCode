<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="60" d:DesignHeight="60"
             x:Class="Furesoft.LowCode.Core.Components.Views.DefaultNodeView"
             ClipToBounds="False">
    <Grid>
        <Rectangle Classes="node" />
        <Label Classes="node" Content="{Binding Label, Mode=TwoWay}" Foreground="{DynamicResource NodeForegroundBrush}" />
    </Grid>
</UserControl>
